{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
     "primary": "#F0DB4F",
     "secondary": "#323330",
     "active": "#ddc948"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\u256d\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf0e7",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\u256d\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{.Icon}} ",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf489 {{ .Name }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "shell",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uea83 {{ .Path }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "git",
          "style": "diamond",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:active{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:secondary{{ end }}",
            "{{ if gt .Ahead 0 }}p:secondary{{ end }}",
            "{{ if gt .Behind 0 }}p:secondary{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235 {{ if .Error }}{{ .Error }} {{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "ruby",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "java",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue624{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "julia",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": " ",
            "charging_icon": "\u21e1 ",
            "discharging_icon": "\u21e3 "
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "battery",
          "style": "diamond",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:active{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:active{{end}}",
            "{{if eq \"Full\" .State.String}}p:secondary{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "3:04 PM"
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf43a {{ .CurrentDate | date .Format }}",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if gt .Code 0 }}\udb85\udea2 \uf00d{{ else }}\udb85\udf1a \uf00c{{ end }} ",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "status",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\u2570\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "❯",
          "foreground": "p:secondary",
          "background": "p:primary",
          "type": "text",
          "style": "diamond"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
